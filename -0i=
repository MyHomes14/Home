                    searchType = "UserQuery",
                    useTag = window.location.search.indexOf("targeturl=ST") > 0,
                    params = [],
                    top100Nms = "";

                if (false)
                    top100Nms = $('.catalog_main_table > .dtList').map(function () { return $(this).data('catalogercod1s'); }).toArray().join(',');

                params.push(wb.stat.wrapParam("Q", query));
                params.push(wb.stat.wrapParam("UQ", userQuery));
                params.push(wb.stat.wrapParam("SRC", resultsCount || 0));
                params.push(wb.stat.wrapParam("SQC", wasTransliterated ? 1 : 0));
                params.push(wb.stat.wrapParam("SET", searchType));
                params.push(wb.stat.wrapParam("SUT", useTag ? 1 : 0));
                params.push(wb.stat.wrapParam("NMS", top100Nms));
                wb.stat.sendEvent("SRE", params);
            }
        };
        var originalUrl = $('div#catalog').attr('data-orig-url');
        if (!originalUrl)
            originalUrl = window.location.href;

        var resultsCount = 0;
        if (productExists == 1) {
            var $wildCat = $("#catalog");
            var $wildCatTable = $wildCat.find('.catalog_main_table');
            var $wildCatContent = $wildCat.find('#catalog-content');
            if ($wildCatTable.length !== 0) {
                var catParams = $wildCatContent.data('catalog-params');
                if (catParams && !catParams.sle && catParams.page === 1)
                        resultsCount = catParams.totalItems;
            }
        }

        sendSearchStatistics(resultsCount);
    

        
            wb.settings.scripts["recommendations"] = "//static.wbstatic.net/j/recommendations.min.js?e16d5529e2227535";
            $.when(wb.utils.script.require(wb.settings.scripts.recommendations, true)).then(function() {
                wb.recommendations.init({"providers":{"disable":0,"nest":1,"retailRocket":2},"locations":{"productCardPosition2":0,"productCardPosition3":1,"productCardPosition4":2,"searchPosition1":3,"searchPosition2":4,"orderConfirmedPosition1":5,"orderConfirmedPosition2":6,"errorPage404Position1":8,"errorPage404Position2":9,"confirmProductPreorderPagePosition1":10,"confirmProductPreorderPagePosition2":11,"errorPage500Position1":12}});
            });
        

});
$(window).load(function() {
    
        if (!wb.settings.isMobileDevice && !wb.settings.disableBackground) {
            var $leftBg = $('.left-bg'),
                $body = $('.main-content'),
                leftBackground = '',
                rightBackground = '',
                backgroundColor = '';
            
            if (leftBackground)
                $leftBg.css('background', leftBackground);
            if (rightBackground)
                $body.css('background', rightBackground);
            if (backgroundColor)
                $leftBg.css('background-color', backgroundColor);
        }
    

    
        (function ($) {
            var $container = $(".j-recent-goods-container");
            var action = loadVisitedGoods;
            
            onScroll($container, '.visited', action);

            function loadVisitedGoods(container) {
                //console.log('loadVisitedGoods');
                $.post("/visitedgoods", { count: container.attr("data-count") }, function (html) {
                    container.html(html);
                    wb.carousel.renderProductPrices(container, true);
                }, "html");
            }

            function onScroll(container, eventNameSpace, action) {
                var eventName = 'scroll' + eventNameSpace;
                $(window).on(eventName, function () {
                    if (!wb.utils.isElementInViewport(container, true))
                        return;

                    $(window).off(eventNameSpace);
                    action(container);
                }).trigger(eventName);
            }
        })(jQuery);
    

(function($){                                      
	                if (!$ || !wb || !wb.utils || !wb.utils.script || !wb.settings || !wb.settings.scripts)
		                return;
	                $.when(wb.utils.script.require(wb.settings.scripts.browser), wb.utils.script.require(wb.settings.scripts.externalStat)).then(function() {
	                    if (!wb.browser || !wb.browser.isBot()){
			                wb.externalStatInit({
				                eventsInfo: '{"counterData":[]}',
				                criteoUidCookieName: 'criteo_uid',
				                criteoMappingKey: '308',
				                processingUrl: '/stats/counters-processing',
				                redirectForCriteo: 'https%3A%2F%2Fwww.wildberries.ru%2Fstats%2Fcriteotrack%3Fuserid%3D%40USERID%40'
			                });
		                }
	                });
                })(jQuery);
});
</script>




