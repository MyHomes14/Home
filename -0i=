        <span class="total {{:pages.length > 0 ? 'many' : 'single'}}">
            <span>{{:totalItems}}</span>
        </span>
        {{/if}}
        {{if totalPages>1}}
        {{if currentPage>1 && totalPages>0}}
        {{for pages}}{{if #data.value == #parent.parent.data.currentPage - 1}}
        <a class='pagination-prev' href="{{:#data.href}}" data-value="{{:#data.value}}"><span class='arrow prev'></span>Предыдущая страница</a>
        {{/if}}{{/for}}
        {{/if}}
        {{for pages}}
        {{if #data.value == #parent.parent.data.currentPage}}
        <span class="pagination-item active">{{:#data.value}}</span>
        {{else #data.value == ''}}
        <span class="pagination-dotes">...</span>
        {{else}}
        <a href="{{:#data.href}}" class="pagination-item" data-value="{{:#data.value}}">{{:#data.value}}</a>
        {{/if}}
        {{/for}}
        {{if currentPage != totalPages && totalPages > 0}}
        {{for pages}}{{if #data.value == #parent.parent.data.currentPage + 1}}
        <a class='pagination-next' href="{{:#data.href}}" data-value="{{:#data.value}}">Следующая страница<span class='arrow next'></span></a>
        {{/if}}{{/for}}
        {{/if}}
        {{/if}}
    </script>
